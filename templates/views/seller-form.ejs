    <script>
        $(function () {
            var
                file_number = 3,
                $form = $('#seller_form'),
                $file_upload = $('#plot_photo', $form),
                $button = $('.submit', $form);

            $button.prop('disabled', 'disabled');

            $file_upload.on('change', function () {
                var number_of_images = $(this)[0].files.length;
                if (number_of_images != file_number) {
                    alert(`You can upload only ${file_number} files.`);
                    $(this).val('');
                    $button.prop('disabled', 'disabled');
                } else {
                    $button.prop('disabled', false);
                }
            });
        });
